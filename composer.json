{
    "name": "mulaidarinull/larascaff",
    "description": "Admin dashboard scaffolding with role and permission",
    "keywords": [
        "laravel",
        "scaffolding",
        "admin",
        "dashboard"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mdnlcorner/larascaff",
    "support": {
        "issues": "https://github.com/mdnlcorner/larascaff/issues",
        "source": "https://github.com/mdnlcorner/larascaff"
    },
    "authors": [
        {
            "name": "Muhamad Abdullah",
            "email": "abdul.240515@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-permission": "^6.9",
        "yajra/laravel-datatables": "^12.0",
        "mavinoo/laravel-batch": "^2.4",
        "gehrisandro/tailwind-merge-laravel": "^1.3",
        "secondnetwork/blade-tabler-icons": "^3.28",
        "spatie/color": "^1.8",
        "illuminate/auth": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/container": "^12.0",
        "illuminate/validation": "^12.0",
        "illuminate/notifications": "^12.0",
        "illuminate/bus": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/session": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/view": "^12.0",
        "illuminate/filesystem": "^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.13"
    },
    "scripts": {
        "pint": "./vendor/bin/pint",
        "stan": "./vendor/bin/phpstan analyse --memory-limit=2G --ansi --no-progress --no-interaction --configuration=phpstan.neon"
    },
    "autoload": {
        "psr-4": {
            "Mulaidarinull\\Larascaff\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mulaidarinull\\Larascaff\\LarascaffServiceProvider"
            ],
            "aliases": {
                "Larascaff": "Mulaidarinull\\Larascaff\\Facades\\Larascaff"
            }
        },
        "branch-alias": {
            "dev-master": "11.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}